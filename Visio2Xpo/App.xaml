<Application x:Class="cvo.buyshans.Visio2Xpo.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ui="clr-namespace:cvo.buyshans.Visio2Xpo.UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <ui:Bootstrapper x:Key="Bootstrapper"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="SaveGlyph" UriSource="\Glyphs\save.png" />
            <BitmapImage x:Key="LoadGlyph" UriSource="\Glyphs\load.png" />
            <BitmapImage x:Key="ExitGlyph" UriSource="\Glyphs\exit.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>